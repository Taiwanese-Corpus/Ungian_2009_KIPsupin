  "資料": [
    {
      "段": [
        [
          "妹妹好，阿婆早。",
          "me-me ho2, a-po5 tsa2."
        ]
      ],
      "篇名": "第一課  好，早"
    },
    {
      "段": [
        [
          "阿姑多謝，阿舅感謝。",
          "a-koo to-sia7, a-ku7 kam2-sia7."
        ]
      ],
      "篇名": "第二課  多謝，感謝"
    },
    {
      "段": [
        [
          "這是籃仔，那是菜。",
          "tse si7 na5-a2, he si7 tshai3."
        ]
      ],
      "篇名": "第三課  這是，那是（一）"
    },
    {
      "段": [
        [
          "這是牛，那是馬。",
          "tse si7 gu5, he si7 be2."
        ]
      ],
      "篇名": "第四課  這是，那是（一）"
    },
    {
      "段": [
        [
          "我有紅柿，你有旺梨，伊有龍眼。",
          "gua2 u7 ang5-khi7, li2 u7 ong5-lai5, i u7 ling5-ging2."
        ]
      ],
      "篇名": "第五課  我有"
    },
    {
      "段": [
        [
          "咱有郵票，恁有圍棋，怹有劍。",
          "lan2 u7 iu5-phio3, lin2 u7 ui5-ki5, in u7 kiam3."
        ]
      ],
      "篇名": "第六課  我們有"
    },
    {
      "段": [
        [
          "我家的糖果，你家的梅子，他家的圓仔。",
          "gun2 tau e5 thng5-a2, lin2 tau e5 m5-a2, in tau e5 inn5-a2."
        ]
      ],
      "篇名": "第七課  我家的"
    }
  ]
}
